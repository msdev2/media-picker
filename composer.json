{
    "name": "msdev2/media-picker",
    "description": "A simple Laravel media picker (file manager) package with a WYSIWYG editor.",
    "keywords": ["laravel", "media", "file manager", "picker", "wysiwyg", "editor"],
    "homepage": "https://github.com/msdev2/media-picker", 
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mragank",
            "email": "mragankshekhar@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": ">=8.0",
        "league/glide-laravel": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0 || ^7.0 || ^8.0 || ^9.0", 
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Msdev2\\MediaPicker\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Msdev2\\MediaPicker\\MediaPickerServiceProvider"
            ]
        }
    }
}